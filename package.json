{
  "name": "node-tests-students",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:ci": "jest --runInBand --ci --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "test:coverage": "jest --coverage",
    "lint": "eslint students/**/src/**/*.js students/**/tests/**/*.js",
    "lint:fix": "eslint students/**/src/**/*.js students/**/tests/**/*.js --fix",
    "complexity": "node quality-checks/analyze-complexity.cjs",
    "complexity:check": "eslint students/**/src/**/*.js --rule 'complexity: [error, 5]' --rule 'max-depth: [error, 3]'"
  },
  "devDependencies": {
    "eslint": "^9.38.0",
    "eslint-plugin-complexity": "^1.0.2",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^29"
  }
}
